							<%include wot_maps.ejs %>	
						<%} else if (game == 'wows') { %>	
							<%include wows_maps.ejs%>
						<%} else if (game == 'lol') { %>
							<%include lol_maps.ejs%>
						<%} else if (game == 'hots') { %>
							<%include hots_maps.ejs%>
						<%} else if (game == 'sc2') { %>
							<%include sc2_maps.ejs%>
						<%} else if (game == 'csgo') { %>
							<%include csgo_maps.ejs%>
						<%} else if (game == 'blitz') { %>	
							<%include blitz_maps.ejs %>	
						<%} else if (game == 'aw') { %>	
							<%include aw_maps.ejs %>	
						<%} %>	
						</select>

					</div>
					
					<%if (game == 'wot') { %>
						<div class="dropdown inline" style="display:none" id="wotbase_map_select_container">
							<select class="selectpicker" id="map_select_wotbase"><%include wotbase_wot_maps.ejs%></select>
						</div>
					<%} %>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="custom_map" data-toggle="tooltip" title="<%=l('Custom map')%>">
						<div class="icon icon-link"></div>
					</button>
					<%if (game == 'wot') { %>
						<br /><input id="use_wotbase" type="checkbox" style="vertical-align:-2px;" /> <span>Use wotbase maps</span>
					<%} %>	
				<hr>

				<h4><%=l('Tools')%></h4>
				<p>
					<div id="contexts">
					<div class="btn-group">
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="ping" data-toggle="tooltip" title="<%=l('Ping map')%>">
						<div class="icon icon-ping_icon"></div>
					</button>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="track" data-toggle="tooltip" title="<%=l('Track cursor')%>">
						<div class="icon icon-recticle_icon"></div>
					</button>
					<%if (game != 'sc2') {%>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="icon" data-toggle="tooltip" title="<%=l('Add icon (click map)')%>">
						<div class="icon icon-<%if (game == 'wows') { %>ship_icon<%} else if (game == 'aw') { %>aw_icon<%} else if (game == 'lol') { %>icon_icon<%} else if (game == 'hots') { %>hots<% } else if (game == 'csgo') { %>csgo<% } else if (game == 'sc2') { %>sc2<% } else { %>tank_icon<%} %>"></div>
					</button>
					<%} else {%>
					<button type="button" class="btn btn-default menu_btn" data-context="icon" aria-label="Left Align" id="icon_terran" data-toggle="tooltip" title="<%=l('Add icon (click map)')%>">
						<div class="icon icon-terran"></div>
					</button>
					<button type="button" class="btn btn-default menu_btn" data-context="icon" aria-label="Left Align" id="icon_zerg" data-toggle="tooltip" title="<%=l('Add icon (click map)')%>">
						<div class="icon icon-zerg"></div>
					</button>
					<button type="button" class="btn btn-default menu_btn" data-context="icon" aria-label="Left Align" id="icon_protoss" data-toggle="tooltip" title="<%=l('Add icon (click map)')%>">
						<div class="icon icon-protoss"></div>
					</button>					
					<%}%>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="draw" data-toggle="tooltip" title="<%=l('Free draw')%>">
						<div class="icon icon-pencil"></div>
					</button>
					
					<%if (game == 'sc2') {%>
					</div>
					<br />
					<div class="btn-group">
					<%}%>
					
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="line" data-toggle="tooltip" title="<%=l('Draw line (hold left mouse)')%>">
						<div class="icon icon-line_icon"></div>
					</button>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="curve" data-toggle="tooltip" title="<%=l('Draw curve (double click to end)')%>">
						<div class="icon icon-curve"></div>
					</button>
					
					<%if (game != 'sc2') {%>
					</div>
					<br />
					<div class="btn-group">
					<%}%>

					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="rectangle" data-toggle="tooltip" title="<%=l('Draw rectangle')%>">
						<div class="icon icon-rectangle"></div>
					</button>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="circle" data-toggle="tooltip" title="<%=l('Draw circle')%>">
						<div class="icon icon-draw_circle_icon"></div>
					</button>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="polygon" data-toggle="tooltip" title="<%=l('Draw polygon')%>">
						<div class="icon icon-polygon"></div>
					</button>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="area" data-toggle="tooltip" title="<%=l('Draw area')%>">
						<div class="icon icon-area"></div>
					</button>
					
					<%if (game == 'sc2') {%>
					</div>
					<br />
					<div class="btn-group">
					<%}%>
					
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="text" data-toggle="tooltip" title="<%=l('Add Text')%>">
						<div class="icon icon-text_icon"></div>
					</button>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="background_text" data-toggle="tooltip" title="<%=l('Add Text with background')%>">
						<div class="icon icon-text_on_background"></div>
					</button>
					
					<%if (game != 'sc2') {%>
					</div>
					<br />
					<div class="btn-group">
					<%}%>
					
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="undo" data-toggle="tooltip" title="<%=l('Undo last action')%>">
						<div class="icon icon-undo"></div>
					</button>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="redo" data-toggle="tooltip" title="<%=l('Redo last action')%>">
						<div class="icon icon-redo"></div>
					</button>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="remove" data-toggle="tooltip" title="<%=l('Delete (click map)/Delete menu')%>">
						<div class="icon icon-bin"></div>
					</button>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="eraser" data-toggle="tooltip" title="<%=l('Erase')%>">
						<div class="icon icon-eraser"></div>
					</button>

					
					<%if (game == 'sc2') {%>
					</div>
					<br />
					<div class="btn-group">
					<%}%>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="note" data-toggle="tooltip" title="<%=l('Add a note')%>">
						<div class="icon icon-note"></div>
					</button>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="select" data-toggle="tooltip" title="<%=l('Select items on the map')%>">
						<div class="icon icon-select"></div>
					</button>					
					<%if (game != 'sc2') {%>
					</div>
					<br />
					<div class="btn-group">
					<%}%>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="ruler" data-toggle="tooltip" title="<%=l('Ruler')%>">
						<div class="icon icon-ruler_icon"></div>
					</button>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="cut" data-toggle="tooltip" title="<%=l('Cut (ctrl-x)')%>">
						<div class="icon icon-cut"></div>
					</button>
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="copy" data-toggle="tooltip" title="<%=l('Copy (ctrl-c)')%>">
						<div class="icon icon-copy"></div>
					</button>			
					<button type="button" class="btn btn-default menu_btn" aria-label="Left Align" id="paste" data-toggle="tooltip" title="<%=l('Paste (press ctrl-v while on the map to paste to a specific location)')%>">
						<div class="icon icon-paste"></div>
					</button>						
					</div>
					</div>					

				<hr>
				
				<div id='icon_context'>
						
				<h4><%=l('Icon options')%> (<span id="icon_counter">0</span>)</h4>
					<div id="icon_label_container">
					<p>	
					<small><%=l('Label')%></small> <input type="text" id="icon_label" />
					<p>
					</div>
					
					<%if (game == 'wot') { %>
						<%include wot_icons.ejs%>
					<%} else if (game == 'wows') { %>
						<%include wows_icons.ejs%>
					<%} else if (game == 'blitz') { %>
						<%include blitz_icons.ejs%>
					<%} else if (game == 'aw') { %>
						<%include aw_icons.ejs%>
					<%} else if (game == 'lol') { %>
						<%include lol_icons.ejs%>
					<%} else if (game == 'hots') { %>
						<%include hots_icons.ejs%>
					<%} else if (game == 'csgo') { %>
						<%include csgo_icons.ejs%>
					<%} %>
					
					<div id="icon_options">
					<select name="colorpicker" id="icon_colorpicker">
						<option value="#ff0000"><%=l('Red')%></option>
						<%if (game == 'aw') { %>
						<option value="#70c5ea"><%=l('Cyan')%></option>
						<% } %>
						<option value="#00ff00"><%=l('Green')%></option>
						<option value="#87CEFA"><%=l('Blue')%></option>
						<%if (game != 'aw') { %>
						<option value="#ffff00"><%=l('Yellow')%></option>
						<% } %>
						<option value="#ffffff"><%=l('White')%></option>
					</select>
					<input type="color">


					<div class="row">
						<div class="col-sm-4">
							<small><%=l('Label size')%></small>
						</div>	
						<div class="col-sm-6">
							<input type="range" id="label_font_size" min="3" value=<%if (game == 'blitz') { %>"15"<%} else {%>"10"<%}%> max="100" step="1" list="label_font_size_ticks">
						</div>	
						<div class="col-sm-2">
							<input id="label_font_size_text" class="slider_text_input" type="text"/>
						</div>	
					</div>	

					<div class="row">
						<div class="col-sm-4">
							<small><%=l('Icon size')%></small>
						</div>	
						<div class="col-sm-6">
							<input type="range" id="icon_size" min="5" value=<%if (game == 'blitz') { %>"35"<%} else if (game == 'wows') { %>"15"<% } else {%>"20"<%}%> max="100" step="1" list="icon_size_ticks">
						</div>	
						<div class="col-sm-2">
							<input id="icon_size_text" class="slider_text_input" type="text"/>
						</div>
					</div>	
					<div class="row">
						<div class="col-sm-4">
							<small><%=l('Brightness')%></small>
						</div>
						<div class="col-sm-6">
							<input type="range" id="icon_brightness" min="-1" value="0" max="1" step="0.05" list="icon_brightness_ticks">
						</div>
						<div class="col-sm-2">
							<input id="icon_brightness_text" class="slider_text_input" type="text"/>
						</div>
					</div>	
					</div>					
				</div>
				
				<%if (game == 'sc2') {%>
				<div id="icon_terran_context">
				<h4><%=l('Terran Icon options')%></h4>
					<%include sc2_terran_icons.ejs%><br/>
				</div>
				<div id="icon_zerg_context">
				<h4><%=l('Zerg Icon options')%></h4>
					<%include sc2_zerg_icons.ejs%><br/>
				</div>
				<div id="icon_protoss_context">
				<h4><%=l('Protoss Icon options')%></h4>
					<%include sc2_protoss_icons.ejs%><br/>
				</div>
				<%}%>

				
