				href="/">
			<% } else if (game == 'wows') { %>
				href="/">
			<% } else if (game == 'blitz') { %>
				href="/blitz.html">
			<% } else if (game == 'lol') { %>
				href="/lol">
			<% } else if (game == 'hots') { %>
				href="/hots">
